<!-- The Lappa robot -->
<mujoco>
    <!-- set options to simulate underwater environment  -->
    <option density="900" viscosity=" 0.0009" />
    <include file="lappa_model.xml"/>
	<worldbody>
		<geom name="floor" pos="0 0 -1" size="2 2 .2" type="plane" conaffinity="1" rgba="1 1 1 1"
			condim="3" />
            <!-- vertical wall in the center of the floor -->
        <geom name="wall" pos="0 2 0" size="1 2 2" type="plane" conaffinity="1" rgba="0 1 0 1"
            condim="3" euler="90 0 90" />
            <!-- horizontal wall in the center of the floor -->
	</worldbody>
</mujoco>