<!-- The Lappa robot -->
<mujoco>
    <!-- set options to simulate underwater environment  -->
    <option density="900" viscosity=" 0.0009" />
    <include file="model.xml"/>
	<worldbody>
        <body name="box" pos="0 0 -1">
            <geom name="ground" type="box" size="1 1 .2" rgba="0 1 0 1" density="100" friction=".5" pos="0 .5 0" />
            <!-- pink rgba -->
            <geom name="pipe" type="box" size="1 .05 .08" density="100" friction=".5" pos="0 .5 .2" rgba="1 .6 .8 1" />
            <geom name="wall" type="box" size="1 .05 .7" density="100" friction=".5" pos="0 1.5 .5" rgba="1 .6 .8 1" />
            <geom name="upper_ground" type="box" size="1 .5 .2" rgba="0 1 0 1" density="100" friction=".5" pos="0 2 1" />
            <geom name="test" type="box" size=".05 .05 .04" density="100" friction=".5" pos="0.1 0 .2" rgba="1 .6 .8 1" />
        </body>
	</worldbody>
</mujoco>