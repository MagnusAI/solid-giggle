<mujoco model="lappa_model">
	<visual>
		<scale contactwidth="0.1" jointwidth="0.05" jointlength=".5" />
	</visual>
	<worldbody>
		<body name="root">
			<freejoint />
			<body name="base">
				<geom name="arm" type="capsule" fromto="-.1 0 0 .1 0 0" size=".012" rgba="1 1 1 1" />
				<body name="a_module" pos="-.1 0 0">
					<site name="a_propeller" pos="-.1 0 -.05" />
					<joint name="a_h2" type="hinge" axis="0 1 0" limited="true" range="-45 45" stiffness=".5" />
					<geom name="a_house" type="cylinder" size=".1 .005" pos="-.1 0 0" rgba="1 0 0 1" />
					<joint name="a_h1" type="hinge" axis="0 0 1" pos="-.1 0 -.05" />
					<body name="a_chamber">
            <site name="a_vacuum" pos="-.1 0 -.1" />
						<site name="a_touch" type="cylinder" size=".105 .015" pos="-.1 0 -.075" />
						<geom name="a_touch" type="cylinder" size=".105 .015" pos="-.1 0 -.075" rgba="0 1 0 .25" mass="0" />
            <geom name="a_gear" type="cylinder" size=".065 .05" pos="-.1 0 -.05" rgba="0.8 0.7 0.2 1" mass="0" />
						<geom name="a_chamber" type="box" size=".045 .045 .05" pos="-.1 0 -.05" rgba="0 0 1 1" />
					</body>
				</body>
				<body name="b_module" pos=".1 0 0">
					<site name="b_propeller" pos=".1 0 -.05" />
					<joint name="h3" type="hinge" axis="1 0 0" limited="true" range="-90 90" pos="-.1 0 0" stiffness=".5" damping="2" />
					<joint name="b_h2" type="hinge" axis="0 1 0" limited="true" range="-45 45" stiffness=".5" />
					<geom name="b_house" type="cylinder" size=".1 .005" pos=".1 0 0" rgba="0 0 1 1" />
					<joint name="b_h1" type="hinge" axis="0 0 1" pos=".1 0 -.05" />
					<body name="b_chamber">
            <site name="b_vacuum" pos=".1 0 -.1" />
						<site name="b_touch" type="cylinder" size=".105 .015" pos=".1 0 -.075" />
						<geom name="b_touch" type="cylinder" size=".105 .015" pos=".1 0 -.075" rgba="0 1 0 .25" mass="0" />
            <geom name="b_gear" type="cylinder" size=".065 .05" pos=".1 0 -.05" rgba="0.8 0.7 0.2 1" mass="0" />
						<geom name="b_chamber" type="box" size=".045 .045 .05" pos=".1 0 -.05" rgba="0 0 1 1" />
					</body>
				</body>
			</body>
		</body>
	</worldbody>
	<sensor>
    <force name="a_vacuum" site="a_vacuum" />
    <force name="b_vacuum" site="b_vacuum" />
    <jointpos name="a_h1" joint="a_h1" />
    <jointpos name="b_h1" joint="b_h1" />
		<jointpos name="a_h2" joint="a_h2" />
		<jointpos name="b_h2" joint="b_h2" />
		<touch name="a_touch" site="a_touch" />
		<touch name="b_touch" site="b_touch" />
	</sensor>
	<actuator>
		<motor name="a_thrust" site="a_propeller" ctrllimited="true" ctrlrange="-1 1" gear="0 0 15" />
		<motor name="b_thrust" site="b_propeller" ctrllimited="true" ctrlrange="-1 1" gear="0 0 15" />
		<adhesion name="a_vacuum" body="a_chamber" ctrlrange="0 1" gain="100" />
		<adhesion name="b_vacuum" body="b_chamber" ctrlrange="0 1" gain="100" />
		<motor name="a_h1" joint="a_h1" gear="1" />
		<motor name="b_h1" joint="b_h1" gear="1" />
	</actuator>
</mujoco>
